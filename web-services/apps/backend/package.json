{
  "name": "@decloud/backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "backend": "bun run ./apps/backend/index.ts",
    "backend:dev": "cd ./apps/backend && bun run --hot index.ts",
    "backend:prod": "bun run ./apps/backend/dist/index.js",
    "build": "bun build ./apps/backend/index.ts --outdir ./apps/backend/dist --target bun --minify",
    "build:backend": "cd ./apps/backend && bun build index.ts --outdir dist --target bun --minify"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@decloud/db": "workspace:*",
    "@decloud/types": "workspace:^"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  
  "dependencies": {
    "@google-cloud/compute": "^5.2.0",
    "@google-cloud/deploy": "^5.1.0",
    "@google-cloud/resource-manager": "^6.1.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/sshpk": "^1.17.4",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.12.0",
    "sshpk": "^1.18.0"
  }
}